    <body style="margin-top: 100px; ">
      <h2>
        <center><b><u><%= title %> </b></u></center>
      </h2>

      <div class="row" id="rusers" style="margin-left: 100px;">
        <% if (rusers != '') { %> <% rusers.forEach((row, index) => { %>

        <div class="col-lg-6 mt-5">
          <span><img src="<%= row.image %>" width="330" height="250" /></span><br />
          <span style="font-weight: bold; font-size: 20px">Event Name:</span>
          <span style="font-size: 18px"><%= row.ename %></span><br />
          <span style="font-weight: bold; font-size: 20px">Student Name:</span>
          <span style="font-size: 18px"><%= row.name %></span><br />
          <span style="font-weight: bold; font-size: 20px">Email ID:</span>
          <span style="font-size: 18px"><%= row.email %></span><br />
          <span style="font-weight: bold; font-size: 20px">Mobile Number:</span>
          <span style="font-size: 18px"><%= row.phone %></span><br />
          <!-- <span style="font-weight: bold; font-size: 20px">Action:</span> -->
          
        </div>
        <% }) %> 
        <% } else { %>
        <h1 class="text-center text-secondary mt-5">No users found</h1>
        <% } %>
      </div>
    </body>

